{
  "definition": {
    "$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
    "actions": {
      "AssetMaintenanceBlobs": {
        "inputs": {
          "variables": [
            {
              "name": "AssetMaintenanceBlobs",
              "type": "array"
            }
          ]
        },
        "runAfter": {},
        "type": "InitializeVariable"
      },
      "CleanupAssetMaintenanceIfMoreThanOneBlob": {
        "actions": {
          "AssetMaintenanceDataCleanupLoop": {
            "actions": {
              "DeleteOldAssetMaintenanceRefData": {
                "inputs": {
                  "headers": {
                    "SkipDeleteIfFileNotFoundOnServer": false
                  },
                  "host": {
                    "connection": {
                      "name": "@parameters('$connections')['azureblob']['connectionId']"
                    }
                  },
                  "method": "delete",
                  "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent(parameters('StorageAccountName')))}/files/@{encodeURIComponent(encodeURIComponent(items('AssetMaintenanceDataCleanupLoop')?['Path']))}"
                },
                "runAfter": {},
                "type": "ApiConnection"
              }
            },
            "foreach": "@body('FilterAssetMaintenanceDataOlderThan7Days')",
            "runAfter": {
              "FilterAssetMaintenanceDataOlderThan7Days": [
                "Succeeded"
              ]
            },
            "type": "Foreach"
          },
          "FilterAssetMaintenanceDataOlderThan7Days": {
            "inputs": {
              "from": "@variables('AssetMaintenanceBlobs')",
              "where": "@less(item()?['LastModified'], subtractFromTime(utcNow(), 7, 'Day'))"
            },
            "runAfter": {},
            "type": "Query"
          }
        },
        "expression": {
          "and": [
            {
              "greater": [
                "@length(variables('AssetMaintenanceBlobs'))",
                1
              ]
            }
          ]
        },
        "runAfter": {
          "SetAssetMaintenanceBlobs": [
            "Succeeded",
            "Skipped"
          ],
          "SetEmptyAssetMaintenanceBlobs": [
            "Succeeded",
            "Skipped"
          ]
        },
        "type": "If"
      },
      "CleanupSensorItemBatchAttributeMappingsIfMoreThanOneBlob": {
        "actions": {
          "FilterSensorItemBatchAttributeMappingsOlderThan7Days": {
            "inputs": {
              "from": "@variables('SensorJobItembatchAttributeMappingsBlobs')",
              "where": "@less(item()?['LastModified'], subtractFromTime(utcNow(), 7, 'Day'))"
            },
            "runAfter": {},
            "type": "Query"
          },
          "SensorJobItemBatchAttributeMappingCleanupLoop": {
            "actions": {
              "DeleteOldSensorItemBatchAttributeMappings": {
                "inputs": {
                  "headers": {
                    "SkipDeleteIfFileNotFoundOnServer": false
                  },
                  "host": {
                    "connection": {
                      "name": "@parameters('$connections')['azureblob']['connectionId']"
                    }
                  },
                  "method": "delete",
                  "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent(parameters('StorageAccountName')))}/files/@{encodeURIComponent(encodeURIComponent(items('SensorJobItemBatchAttributeMappingCleanupLoop')?['Path']))}"
                },
                "runAfter": {},
                "type": "ApiConnection"
              }
            },
            "foreach": "@body('FilterSensorItemBatchAttributeMappingsOlderThan7Days')",
            "runAfter": {
              "FilterSensorItemBatchAttributeMappingsOlderThan7Days": [
                "Succeeded"
              ]
            },
            "type": "Foreach"
          }
        },
        "expression": {
          "and": [
            {
              "greater": [
                "@length(variables('SensorJobItembatchAttributeMappingsBlobs'))",
                1
              ]
            }
          ]
        },
        "runAfter": {
          "SetEmptySensorJobItembatchAttributeMappingsBlobs": [
            "Succeeded",
            "Skipped"
          ],
          "SetSensorJobItembatchAttributeMappingsBlobs": [
            "Succeeded",
            "Skipped"
          ]
        },
        "type": "If"
      },
      "CleanupSensorJobExecutionsIfMoreThanOneBlob": {
        "actions": {
          "FilterSensorJobExecutionsBlobsOlderThan7Days": {
            "inputs": {
              "from": "@variables('SensorJobExecutionsBlobs')",
              "where": "@less(item()?['LastModified'], subtractFromTime(utcNow(), 7, 'Day'))"
            },
            "runAfter": {},
            "type": "Query"
          },
          "SensorJobExectionsCleanupLoop": {
            "actions": {
              "DeleteOldSensorJobExecutions": {
                "inputs": {
                  "headers": {
                    "SkipDeleteIfFileNotFoundOnServer": false
                  },
                  "host": {
                    "connection": {
                      "name": "@parameters('$connections')['azureblob']['connectionId']"
                    }
                  },
                  "method": "delete",
                  "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent(parameters('StorageAccountName')))}/files/@{encodeURIComponent(encodeURIComponent(items('SensorJobExectionsCleanupLoop')?['Path']))}"
                },
                "runAfter": {},
                "type": "ApiConnection"
              }
            },
            "foreach": "@body('FilterSensorJobExecutionsBlobsOlderThan7Days')",
            "runAfter": {
              "FilterSensorJobExecutionsBlobsOlderThan7Days": [
                "Succeeded"
              ]
            },
            "type": "Foreach"
          }
        },
        "expression": {
          "and": [
            {
              "greater": [
                "@length(variables('SensorJobExecutionsBlobs'))",
                1
              ]
            }
          ]
        },
        "runAfter": {
          "SetEmptySensorJobExecutionsBlobs": [
            "Succeeded",
            "Skipped"
          ],
          "SetSensorJobExecutionsBlobs": [
            "Succeeded",
            "Skipped"
          ]
        },
        "type": "If"
      },
      "CleanupSensorJobsIfMoreThanOneBlob": {
        "actions": {
          "FilterSensorJobsBlobsOlderThan7Days": {
            "inputs": {
              "from": "@variables('SensorJobsBlobs')",
              "where": "@less(item()?['LastModified'], subtractFromTime(utcNow(), 7, 'Day'))"
            },
            "runAfter": {},
            "type": "Query"
          },
          "SensorJobCleanupLoop": {
            "actions": {
              "DeleteOldSensorJobs": {
                "inputs": {
                  "headers": {
                    "SkipDeleteIfFileNotFoundOnServer": false
                  },
                  "host": {
                    "connection": {
                      "name": "@parameters('$connections')['azureblob']['connectionId']"
                    }
                  },
                  "method": "delete",
                  "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent(parameters('StorageAccountName')))}/files/@{encodeURIComponent(encodeURIComponent(items('SensorJobCleanupLoop')?['Path']))}"
                },
                "runAfter": {},
                "type": "ApiConnection"
              }
            },
            "foreach": "@body('FilterSensorJobsBlobsOlderThan7Days')",
            "runAfter": {
              "FilterSensorJobsBlobsOlderThan7Days": [
                "Succeeded"
              ]
            },
            "type": "Foreach"
          }
        },
        "expression": {
          "and": [
            {
              "greater": [
                "@length(variables('SensorJobsBlobs'))",
                1
              ]
            }
          ]
        },
        "runAfter": {
          "SetEmptySensorJobsBlobs": [
            "Succeeded",
            "Skipped"
          ],
          "SetSensorJobsBlobs": [
            "Succeeded",
            "Skipped"
          ]
        },
        "type": "If"
      },
      "ListAllAssetMaintenanceBlobs": {
        "inputs": {
          "host": {
            "connection": {
              "name": "@parameters('$connections')['azureblob']['connectionId']"
            }
          },
          "method": "get",
          "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent(parameters('StorageAccountName')))}/foldersV2/@{encodeURIComponent(encodeURIComponent('/sensorintelligencereferencedata/assetmaintenancedata'))}",
          "queries": {
            "nextPageMarker": "",
            "useFlatListing": false
          }
        },
        "runAfter": {
          "AssetMaintenanceBlobs": [
            "Succeeded"
          ]
        },
        "type": "ApiConnection"
      },
      "ListAllSensorJobExecutionsBlobs": {
        "inputs": {
          "host": {
            "connection": {
              "name": "@parameters('$connections')['azureblob']['connectionId']"
            }
          },
          "method": "get",
          "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent(parameters('StorageAccountName')))}/foldersV2/@{encodeURIComponent(encodeURIComponent('/sensorintelligencereferencedata/sensorjobexecutions'))}",
          "queries": {
            "nextPageMarker": "",
            "useFlatListing": false
          }
        },
        "runAfter": {
          "SensorJobExecutionsBlobs": [
            "Succeeded"
          ]
        },
        "type": "ApiConnection"
      },
      "ListAllSensorJobItembatchAttributeMappings": {
        "inputs": {
          "host": {
            "connection": {
              "name": "@parameters('$connections')['azureblob']['connectionId']"
            }
          },
          "method": "get",
          "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent(parameters('StorageAccountName')))}/foldersV2/@{encodeURIComponent(encodeURIComponent('/sensorintelligencereferencedata/sensorjobbatchattributes'))}",
          "queries": {
            "nextPageMarker": "",
            "useFlatListing": false
          }
        },
        "runAfter": {
          "SensorJobItembatchAttributeMappingsBlobs": [
            "Succeeded"
          ]
        },
        "type": "ApiConnection"
      },
      "ListAllSensorJobsBlobs": {
        "inputs": {
          "host": {
            "connection": {
              "name": "@parameters('$connections')['azureblob']['connectionId']"
            }
          },
          "method": "get",
          "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent(parameters('StorageAccountName')))}/foldersV2/@{encodeURIComponent(encodeURIComponent('/sensorintelligencereferencedata/sensorjobs'))}",
          "queries": {
            "nextPageMarker": "",
            "useFlatListing": false
          }
        },
        "runAfter": {
          "SensorJobsBlobs": [
            "Succeeded"
          ]
        },
        "type": "ApiConnection"
      },
      "SensorJobExecutionsBlobs": {
        "inputs": {
          "variables": [
            {
              "name": "SensorJobExecutionsBlobs",
              "type": "array"
            }
          ]
        },
        "runAfter": {},
        "type": "InitializeVariable"
      },
      "SensorJobItembatchAttributeMappingsBlobs": {
        "inputs": {
          "variables": [
            {
              "name": "SensorJobItembatchAttributeMappingsBlobs",
              "type": "array"
            }
          ]
        },
        "runAfter": {},
        "type": "InitializeVariable"
      },
      "SensorJobsBlobs": {
        "inputs": {
          "variables": [
            {
              "name": "SensorJobsBlobs",
              "type": "array"
            }
          ]
        },
        "runAfter": {},
        "type": "InitializeVariable"
      },
      "SetAssetMaintenanceBlobs": {
        "inputs": {
          "name": "AssetMaintenanceBlobs",
          "value": "@body('ListAllAssetMaintenanceBlobs')?['value']"
        },
        "runAfter": {
          "ListAllAssetMaintenanceBlobs": [
            "Succeeded"
          ]
        },
        "type": "SetVariable"
      },
      "SetEmptyAssetMaintenanceBlobs": {
        "inputs": {
          "name": "AssetMaintenanceBlobs",
          "value": []
        },
        "runAfter": {
          "ListAllAssetMaintenanceBlobs": [
            "Failed"
          ]
        },
        "type": "SetVariable"
      },
      "SetEmptySensorJobExecutionsBlobs": {
        "inputs": {
          "name": "SensorJobExecutionsBlobs",
          "value": []
        },
        "runAfter": {
          "ListAllSensorJobExecutionsBlobs": [
            "Failed"
          ]
        },
        "type": "SetVariable"
      },
      "SetEmptySensorJobItembatchAttributeMappingsBlobs": {
        "inputs": {
          "name": "SensorJobItembatchAttributeMappingsBlobs",
          "value": []
        },
        "runAfter": {
          "ListAllSensorJobItembatchAttributeMappings": [
            "Failed"
          ]
        },
        "type": "SetVariable"
      },
      "SetEmptySensorJobsBlobs": {
        "inputs": {
          "name": "SensorJobsBlobs",
          "value": []
        },
        "runAfter": {
          "ListAllSensorJobsBlobs": [
            "Failed"
          ]
        },
        "type": "SetVariable"
      },
      "SetSensorJobExecutionsBlobs": {
        "inputs": {
          "name": "SensorJobExecutionsBlobs",
          "value": "@body('ListAllSensorJobExecutionsBlobs')?['value']"
        },
        "runAfter": {
          "ListAllSensorJobExecutionsBlobs": [
            "Succeeded"
          ]
        },
        "type": "SetVariable"
      },
      "SetSensorJobItembatchAttributeMappingsBlobs": {
        "inputs": {
          "name": "SensorJobItembatchAttributeMappingsBlobs",
          "value": "@body('ListAllSensorJobItembatchAttributeMappings')?['value']"
        },
        "runAfter": {
          "ListAllSensorJobItembatchAttributeMappings": [
            "Succeeded"
          ]
        },
        "type": "SetVariable"
      },
      "SetSensorJobsBlobs": {
        "inputs": {
          "name": "SensorJobsBlobs",
          "value": "@body('ListAllSensorJobsBlobs')?['value']"
        },
        "runAfter": {
          "ListAllSensorJobsBlobs": [
            "Succeeded"
          ]
        },
        "type": "SetVariable"
      }
    },
    "contentVersion": "1.0.0.0",
    "outputs": {},
    "parameters": {
      "$connections": {
        "defaultValue": {},
        "type": "Object"
      },
      "StorageAccountName": {
        "defaultValue": "",
        "type": "String"
      }
    },
    "triggers": {
      "Recurrence": {
        "evaluatedRecurrence": {
          "frequency": "Day",
          "interval": 7
        },
        "recurrence": {
          "frequency": "Day",
          "interval": 7
        },
        "type": "Recurrence"
      }
    }
  },
  "parameters": {}
}
